<div class="container">

  <h1>Welcome to Panda Flow, <%= current_username %>!</h1>
  <ul>
    <% @questions.each do |question| %>
    <li><a href="/questions/<%=question.id%>"><%= question.title %></a> <%= question.votes.pluck("point_value").sum %></li>
    <% end %>

  </ul>
  <a href="/questions/new">Ask a Question</a>
</div>
