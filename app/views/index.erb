<div class="container">
  <h1>Welcome to Panda Flow <%= current_username %>!</h1>
  <ul>
    <% @questions.each do |question| %>
    <li>
    <a href="/questions/<%=question.id%>"><%= question.votes.pluck("point_value").sum %> <%= question.title %></a>
    <% end %>
    </li>
  </ul>
</div>

<a href="/questions/new">Ask a Question</a>
